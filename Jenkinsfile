pipeline {
    agent any

    stages {
        stage("init"){
            steps{
                script{
                    echo "initialisation of project"
                }
            }
            
        }

        stage("build"){
            steps{
                script{
                    echo "build the project"
                }
            }
        }

        stage("test"){
            
            steps{
                script{
                    echo "test the project"
                }
            }
        }

        stage("deploy"){
            steps{
                script{
                    echo "deploy the project"
                }
            }
        }
    }
}